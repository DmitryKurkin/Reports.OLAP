<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:bl="clr-namespace:Reporting.BusinessLogic;assembly=Reporting.BusinessLogic">

    <DataTemplate DataType="{x:Type bl:DataSetConfigurationFile}">
        <Grid Margin="3">
            <TextBlock Text="{Binding DisplayName}"/>
        </Grid>
    </DataTemplate>

    <DataTemplate DataType="{x:Type bl:CubeConfigurationFile}">
        <Grid Margin="3">
            <TextBlock Text="{Binding DisplayName}"/>
        </Grid>
    </DataTemplate>
</ResourceDictionary>